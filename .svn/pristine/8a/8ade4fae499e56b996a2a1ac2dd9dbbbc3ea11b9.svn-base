.container{
    width: 100%;
    /*height: 800px;*/
    /*margin:0px auto;*/
    margin-top:130px;//设置搜索top上边距
}
.search{
    /*border:1px solid #85c807;*/
    border:  1px solid #0e5f35;
    /*position: fixed;*/
    top:0px;
    /*left:0px;*/
    width: 506px;
    height: 40px;
    margin:0 auto;
}
.s_con{
    position: relative;
    float: left;
    display: block;
    width: 390px;
    height: 40px;
}
.s_con input{
    display: inline-block;
    border: 0px;
    width: 100%;
    height: 40px;
    line-height: 40px;
    padding:0; /*这个属性跟easy-ui.css的冲突，所以些处覆盖掉*/
}
.s_con .clear{
    display: none;
    position: absolute;
    right: 0px;
    top:6px;
    content: "";
    width: 25px;
    height: 25px;
    /*background-image:url('../images/del.svg');*/
    background-size: 25px 25px;
    cursor:pointer;

}
.s_btn{
    float: left;
    display: block;
    /*background-color: #85c807;*/
    background-color: #0e5f35 !important;
    width: 116px;
    height: 40px;
    line-height: 40px;
    color: #fff;
    font-size: 14px;
    text-align: center;
    cursor: pointer;

}


.search-g{
    width: 506px;
    background-color: #0e5f35;
    padding: 0px;
    padding-top: 0.5px;
    box-sizing: border-box;
    margin: 0 auto
}

.search-g .s_con-g{
    display: inline-block;
    box-sizing: border-box;
    vertical-align: middle;
}

.search-g .s_con-g>input{
    display: inline-block;
    width: 403px;
    height: 40px;
    outline: none;
    padding-left: 10px;
    font-size: 14px;
    box-sizing: border-box;
}

.search-g .s_btn-g{
    width: 96px;
    height: 40px;
    line-height: 40px;
    text-align: center;
    display: inline-block;
    box-sizing: border-box;
    vertical-align: middle;
    font-size: 16px;
    color: white;
    cursor: pointer;
}