}
/*2017-8-2控件右上角小红点 */
.sde-right-point {
	position:relative;
}
.sde-right-point::before {
	content:' ';
	position:absolute;
	background-color:red;
	color:red;
	top:13px;
	left:4px;
	font-weight:800;
	font-size:22px;
	padding:2px;
	border-radius:50%;
}
.sde-right-point[style="color:#808080"]::before {
	content:'';
}
.none-point::before {
	content:'';
	background-color:transparent;
}
@media print {
	.sde-right::before {
		display:none;
	}
