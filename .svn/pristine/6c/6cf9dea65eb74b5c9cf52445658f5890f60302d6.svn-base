	border:1px #ddd solid;
	padding:5px;
	margin-bottom:5px;
}
.span-revise-black {
	color:#000000;
	text-decoration:none !important;
}
.span-revise-edit {
	color:#FF0101;
	text-decoration:underline !important;
}
.span-revise > .span-revise-edit {
	color:#FF0101;
	text-decoration:none !important;
}
.span-revise {
