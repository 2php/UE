.sde-validatebox{
	display: inline-block;
	padding: 0 5px 0 14px;
	width: 78px;
	height: 30px;
	line-height: 19px;
	position: absolute;
	left: 8px;
	top: -1px;
	background: url(../../dialogs/img/tip.png) no-repeat 0 0;
	background-size: 83px;
	font-size: 10px;
	font-weight: bold;
	color: white;
	z-index: 20;
}

.sde-tip{
	position: absolute;
	top: -4px;
	left:0;
	display: flex;
}
.sde-tips{
	display: inline-block;
	padding: 0 5px 0 14px;
	height: 18px;
	left: 12px;
	top: -2px;
	font-size: 10px;
	font-weight: bold;
	color: white;
	z-index: 20;
	transform-origin: 100% 100%;
	-webkit-transform-origin: 100% 100%;
	-webkit-transition: all 1s ease-in-out 0s;
	-o-transition: all 1s ease-in-out 0s;
	-moz-transition: all 1s ease-in-out 0s;
	transition: all 1s ease-in-out 0s;
}
.sde-tip .tipbox{
	width: 64px;
	height: 30px;
	line-height: 19px;
	left: 8px;
	top: -1px;
	background: url(../../dialogs/img/tip.png) no-repeat 0 0;
	background-size: 83px;
}
.sde-tip .info{
	background: url(./default/images/info.png) no-repeat;
	background-color: white;
	background-size: 18px;
}
.sde-tip .warning{
	background: url(./default/images/warning.png) no-repeat;
	background-color: white;
	background-size: 18px;
}
.sde-tip .error{
	background: url(./default/images/error.png) no-repeat;
	background-color: white;
	background-size: 18px;
}
.sde-tip .success{
	background: url(./default/images/success.png) no-repeat;
	background-color: white;
	background-size: 18px;
}
.measureH{
	width: 100%;
	height: 277mm;
	position: absolute;
	opacity: 0.5;
	filter:alpha(opacity=50);
}

/*重写下拉框滚动条样式*/
/*定义滚动条高宽及背景 高宽分别对应横竖滚动条的尺寸*/
::-webkit-scrollbar
{
	width: 6px;
	height:10px;
	background-color: #F5F5F5;
}

/*定义滚动条轨道 内阴影+圆角*/
::-webkit-scrollbar-track
{
	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
	border-radius: 4px;
	background-color: #F5F5F5;
}

/*定义滑块 内阴影+圆角*/
::-webkit-scrollbar-thumb
{
	border-radius: 4px;
	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);
	background-color: #555;
}
