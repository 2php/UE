.dock{
	position:relative;/*height:0px;*/
	text-align:center;
	display: flex;
	justify-content:center;
}
.dock-container{
/*	position:absolute;
	height:40px;
	padding-left:20px;*/
	margin-top: 40px;
	left: auto !important;
    width: auto !important;
}
.dock-container:last-child{
	margin-bottom: 10px;
}
a.dock-item ,a.dock-item2{
	display: inline-block;
	/*display:block;*/
	/*width:80px;*/
	color:#000;
/*	position:absolute;
	top:0px;*/
	margin: 0 20px;
	text-align:center;
	text-decoration:none;
	font:bold 12px Arial, Helvetica, sans-serif;
	cursor: pointer;
}
.dock-item img,.dock-item2 img{border:none;margin:10px 0px 0px;width:80%;height: 64px;}
.dock-item span,.dock-item2 span{display:none;/*padding-left:20px;*/}
.dock-item:hover,.dock-item2:hover{
	background: #d7d9d8;
	border-radius: 13px;
	transform: scale(1.5);
	/*box-shadow: 50px 50px 50px rgba(255,255,255,0.8);*/
	/*width:100px;*/
	/*height: 100px;*/
}
.dyclass{background: #d7d9d8; border-radius: 13px;transform: scale(1.5);}
