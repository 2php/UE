/*以下是指标样式*/
.index{

	position: absolute;
	margin:0 auto;
	width:100%;
	/*height:90px;*/
	/*border:1px solid #F00;*/
	bottom:24px;
}
.indexIcon{
	margin:0 auto;
	text-align: center;
	line-height:90px;/*垂直居中用*/
	color:#ffffff;
	font-size:30px ;
	width:90px;
	height:90px;
	border-radius: 13px;
	box-shadow: 0px 7px 15px 7px #888888 ;

	background: #8dd108;
	background: -moz-linear-gradient(top,  #8dd108 0%, #528c02 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#8dd108), color-stop(100%,#528c02));
	background: -webkit-linear-gradient(top,  #8dd108 0%,#528c02 100%);
	background: -o-linear-gradient(top,  #8dd108 0%,#528c02 100%);
	background: -ms-linear-gradient(top,  #8dd108 0%,#528c02 100%);
	/*background: linear-gradient(to bottom,  #8dd108 0%,#528c02 100%);*/
	background: linear-gradient(to bottom,  #06b55a 0%,#266f49 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#8dd108', endColorstr='#528c02',GradientType=0 );
}
.iconText_index{
	text-align: center;
	display:block;
	font:bold 24px "微软雅黑";
	/*color:#528c01;*/
	color: #0e5f35;
	margin-top: 20px;
}
.index_ul{
	display: table;
	margin:0 auto;
	text-align: center;
	padding:0;
}
.index_li{
	float: left;
	margin-left: 50px;
	padding:0;
	/*line-height: 60px;*/
	font-size: 20px;
	list-style-type: none;
}
.logo{
	/*position: absolute;*/
	/*top:25px;*/
	/*left:25px;*/
	position:absolute; float:left; z-index:100;
	display: block;
}
.sysToolbar_ul{
	display: table;
	margin:0 auto;
	text-align: center;
	padding:0;
}
.sysToolbar_li{
	float: left;
	margin-left: 50px;
	padding:0;
	/*line-height: 60px;*/
	font-size: 20px;
	list-style-type: none;


}
.sysToolbarDiv{
	position:absolute;
	float:right;
	z-index:100;
	right:0;
	top:40px;
	right: 25px;

}
.sysToolbarDiv .sysToolbar_ul .sysToolbar_li img,.sysToolbarDiv .sysToolbar_ul .sysToolbar_li span{
	cursor:pointer;
}
.sysToolbarDiv span{
	font-family: "微软雅黑";
	font-size: 14px;
	margin-left: 5px;
	bottom:0px;
}
/*模糊化效果实现*/
.blur{
	-webkit-filter: blur(3px);
	-moz-filter: blur(3px);
	-o-filter: blur(3px);
	-ms-filter: blur(3px);
	filter: blur(3px);
}